// https://zed.dev/docs/key-bindings â€¢ `zed: open default keymap`
[
  {
    "bindings": {
      "shift-escape": null,
      "ctrl-z": "workspace::ToggleZoom"
    }
  },
  {
    "context": "(VimControl && !menu) || EmptyPane || SharedScreen",
    "bindings": {
      ", e": "file_finder::Toggle",
      ", f": "tab_switcher::Toggle"
    }
  },
  {
    "context": "vim_mode == normal",
    "bindings": {
      "g r": "editor::Rename",
      "z enter": "editor::ScrollCursorTop"
    }
  },
  {
    "context": "vim_mode == visual",
    "bindings": {
      ", s": "editor::SortLinesCaseInsensitive",
      "v": "editor::SelectLargerSyntaxNode",
      "V": "editor::SelectSmallerSyntaxNode"
    }
  }
]
